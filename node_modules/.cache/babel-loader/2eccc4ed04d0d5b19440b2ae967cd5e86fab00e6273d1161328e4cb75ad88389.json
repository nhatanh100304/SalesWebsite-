{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport items from '../data/items';\nimport cart from '../data/cart';\nexport default {\n  data() {\n    return {\n      product: {},\n      cart\n    };\n  },\n  mounted() {\n    let id = this.$route.params.id;\n    this.product = items.find(x => x.id == id);\n  },\n  methods: {\n    addToCard(product) {\n      const infoProduct = this.card.find(item => item.id == product.id);\n      if (infoProduct) {\n        infoProduct.quantity + 1;\n      } else {\n        const newProduct = {\n          ...product,\n          quantity: 1\n        };\n        this.cart.push(newProduct);\n      }\n    }\n  }\n};","map":{"version":3,"names":["items","cart","data","product","mounted","id","$route","params","find","x","methods","addToCard","infoProduct","card","item","quantity","newProduct","push"],"sources":["D:\\Thá»© 4\\food\\src\\components\\DetailCard.vue"],"sourcesContent":["<template>\r\n  <div class=\"menu\">\r\n            <h2 class=\"menu-title\">Taste Our Foods & Enjoy</h2>\r\n            <div class=\"menu-carousel\">\r\n                <div class=\"menu-item\">\r\n                    <img :src=\"product.image\" alt=\"Pumpkin Spice Juice\">\r\n                    <div class=\"menu-item-info\">\r\n                        <h3> {{product.title}} </h3>\r\n                        <p> {{product.description}} </p>\r\n                        <p> {{product.price}} </p>\r\n                    </div>\r\n                   \r\n                </div>\r\n                 <div>\r\n                        <button @click=\"addToCard(product)\"> Add to card</button>\r\n                    </div>\r\n                \r\n                \r\n                \r\n            </div>\r\n            <div class=\"category-nav\">\r\n                <ul>\r\n                    <li>\r\n                        <a href=\"#\">\r\n                            <i class=\"fa fa-coffee\"></i>\r\n                            <span>Breakfast</span>\r\n                        </a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"#\">\r\n                            <i class=\"fa fa-utensils\"></i>\r\n                            <span>Lunch</span>\r\n                        </a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"#\">\r\n                            <i class=\"fa fa-drumstick-bite\"></i>\r\n                            <span>Dinner</span>\r\n                        </a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"#\">\r\n                            <i class=\"fa fa-birthday-cake\"></i>\r\n                            <span>Desserts</span>\r\n                        </a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"#\">\r\n                            <i class=\"fa fa-cocktail\"></i>\r\n                            <span>Drinks</span>\r\n                        </a>\r\n                    </li>\r\n                </ul>\r\n            </div>\r\n        </div>\r\n</template>\r\n\r\n<script>\r\nimport items from '../data/items'\r\nimport cart from '../data/cart'\r\nexport default {\r\n    data(){\r\n        return{\r\n            product:{},\r\n            cart\r\n        }\r\n    },\r\nmounted(){\r\n    let id=this.$route.params.id\r\n    this.product = items.find(x=>x.id==id)\r\n},\r\nmethods:{\r\n    addToCard(product){\r\n        const infoProduct=this.card.find(item => item.id ==product.id)\r\n        if(infoProduct){\r\n            infoProduct.quantity+1\r\n        }\r\n        else\r\n        {\r\n            const newProduct = {...product,quantity:1}\r\n            this.cart.push(newProduct)\r\n        }\r\n    }\r\n}\r\n}\r\n</script>\r\n\r\n<style>\r\n.menu-item {\r\n    position: relative;\r\n    flex: 0 0 25%;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n    border-radius: 8px;\r\n    margin: 0 10px;\r\n    overflow: hidden;\r\n    transition: transform 0.3s ease;\r\n}\r\n\r\n.menu-item img {\r\n    width: 30%;\r\n    transition: transform 0.3s ease;\r\n}\r\n\r\n.menu-item-info {\r\n    position: absolute;\r\n    bottom: 0;\r\n    left: 0;\r\n    right: 0;\r\n    background-color: rgba(0, 0, 0, 0.7);\r\n    color: #fff;\r\n    text-align: left;\r\n    padding: 20px;\r\n    opacity: 0;\r\n    transition: opacity 0.3s ease;\r\n}\r\n\r\n.menu-item:hover .menu-item-info {\r\n    opacity: 1;\r\n}\r\n\r\n.menu-item:hover img {\r\n    transform: scale(1.1);\r\n}\r\n\r\n.menu-item-info h3 {\r\n    font-size: 18px;\r\n    margin: 0 0 10px;\r\n}\r\n\r\n.menu-item-info p {\r\n    margin: 0;\r\n    color: #ccc;\r\n}\r\n\r\n.menu-navigation {\r\n    position: absolute;\r\n    top: 50%;\r\n    width: 100%;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    transform: translateY(-50%);\r\n}\r\n\r\n.menu-navigation i {\r\n    background-color: rgba(0, 0, 0, 0.5);\r\n    color: #fff;\r\n    padding: 10px;\r\n    border-radius: 50%;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s ease;\r\n}\r\n\r\n.menu-navigation i:hover {\r\n    background-color: #fcb034;\r\n}\r\n\r\n.category-nav {\r\n    margin: 20px 0;\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n\r\n.category-nav ul {\r\n    list-style: none;\r\n    padding: 0;\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n\r\n.category-nav ul li {\r\n    margin: 0 20px;\r\n}\r\n\r\n.category-nav ul li a {\r\n    text-decoration: none;\r\n    color: #666;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    font-weight: bold;\r\n    transition: color 0.3s ease;\r\n}\r\n\r\n.category-nav ul li a:hover {\r\n    color: #000;\r\n}\r\n\r\n.category-nav ul li a span {\r\n    margin-top: 5px;\r\n}\r\n\r\n.category-nav ul li a i {\r\n    font-size: 24px;\r\n}\r\n</style>"],"mappings":";AA0DA,OAAOA,KAAI,MAAO,eAAc;AAChC,OAAOC,IAAG,MAAO,cAAa;AAC9B,eAAe