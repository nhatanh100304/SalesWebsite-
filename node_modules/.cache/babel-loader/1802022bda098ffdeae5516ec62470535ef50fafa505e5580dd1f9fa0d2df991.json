\r\n      const productInCart = this.cart.find(item => item.id === product.id);\r\n\r\n      if (productInCart) {\r\n        if (productInCart.quantity < product.quantity) {\r\n          productInCart.quantity += 1;  // Tăng số lượng sản phẩm trong giỏ\r\n          alert(\"Sản phẩm đã được thêm vào giỏ hàng.\");\r\n        } else {\r\n          alert(\"Không đủ số lượng trong kho!\");\r\n        }\r\n      } else {\r\n        if (product.quantity > 0) {\r\n          const newProduct = { ...product, quantity: 1 };\r\n          this.cart.push(newProduct);  // Thêm sản phẩm mới vào giỏ hàng\r\n          alert(\"Sản phẩm đã được thêm vào giỏ hàng.\");\r\n        } else {\r\n          alert(\"Không đủ số lượng trong kho!\");\r\n        }\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.menu {\r\n  text-align: center;\r\n  padding: 30px;\r\n  background-color: #f9f9f9;\r\n}\r\n\r\n.menu-title {\r\n  font-size: 36px;\r\n  font-weight: bold;\r\n  margin-bottom: 20px;\r\n  color: #333;\r\n  text-transform: uppercase;\r\n}\r\n\r\n.menu-carousel {\r\n  display: flex;\r\n  justify-content: center;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n}\r\n\r\n.menu-item {\r\n  position: relative;\r\n  width: 300px;\r\n  height: 400px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n  border-radius: 10px;\r\n  overflow: hidden;\r\n  background-color: #fff;\r\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\r\n}\r\n\r\n.menu-item img {\r\n  width: 100%;\r\n  height: 60%;\r\n  object-fit: cover;\r\n  transition: transform 0.3s ease;\r\n}\r\n\r\n.menu-item-info {\r\n  position: absolute;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  background-color: rgba(0, 0, 0, 0.7);\r\n  color: white;\r\n  padding: 20px;\r\n}\r\n\r\n.menu-item:hover {\r\n  transform: translateY(-10px);\r\n  box-shadow: 0 8px 20px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n.menu-item:hover img {\r\n  transform: scale(1.05);\r\n}\r\n\r\n.menu-item-info h3 {\r\n  font-size: 22px;\r\n  margin: 0 0 10px;\r\n  font-weight: bold;\r\n}\r\n\r\n.menu-item-info p {\r\n  margin: 0;\r\n  color: #ccc;\r\n}\r\n\r\n.price {\r\n  color: #fcb034;\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n  margin-top: 10px;\r\n}\r\n\r\n.add-to-cart {\r\n  text-align: center;\r\n  margin-top: 15px;\r\n}\r\n\r\n.add-to-cart button {\r\n  background-color: #fcb034;\r\n  color: white;\r\n  font-size: 16px;\r\n  padding: 10px 20px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.add-to-cart button:hover {\r\n  background-color: #d99026;\r\n}\r\n\r\n.add-to-cart button:disabled {\r\n  background-color: #e0e0e0;\r\n  cursor: not-allowed;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .menu-carousel {\r\n    flex-direction: column;\r\n    align-items: center;\r\n  }\r\n\r\n  .menu-item {\r\n    width: 80%;\r\n    margin: 0 auto 20px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAM;;EAEVA,KAAK,EAAC;AAAe;;EAH9BC,GAAA;EAIWD,KAAK,EAAC;;mBAJjB;;EAMaA,KAAK,EAAC;AAAgB;;EAGtBA,KAAK,EAAC;AAAO;;EAIfA,KAAK,EAAC;AAAa;mBAb9B;;uBACEE,mBAAA,CAkBM,OAlBNC,UAkBM,G,0BAjBJC,mBAAA,CAAmD;IAA/CJ,KAAK,EAAC;EAAY,GAAC,yBAAuB,sBAC9CI,mBAAA,CAeM,OAfNC,UAeM,GAdyBC,KAAA,CAAAC,OAAO,I,cAApCL,mBAAA,CAOM,OAPNM,UAOM,GANJJ,mBAAA,CAA8C;IAAxCK,GAAG,EAAEH,KAAA,CAAAC,OAAO,CAACG,KAAK;IAAEC,GAAG,EAAC;0BALtCC,UAAA,GAMQR,mBAAA,CAIM,OAJNS,UAIM,GAHJT,mBAAA,CAA4B,YAAAU,gBAAA,CAArBR,KAAA,CAAAC,OAAO,CAACQ,KAAK,kBACpBX,mBAAA,CAAgC,WAAAU,gBAAA,CAA1BR,KAAA,CAAAC,OAAO,CAACS,WAAW,kBACzBZ,mBAAA,CAA6D,KAA7Da,UAA6D,EAAAH,gBAAA,CAAzCR,KAAA,CAAAC,OAAO,CAACW,KAAK,CAACC,cAAc,MAAK,MAAI,gB,OATnEC,mBAAA,gBAaMhB,mBAAA,CAIM,OAJNiB,UAIM,GAHJjB,mBAAA,CAES;IAFAkB,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEC,QAAA,CAAAC,SAAS,CAACpB,KAAA,CAAAC,OAAO;IAAIoB,QAAQ,EAAEF,QAAA,CAAAG,YAAY,CAACtB,KAAA,CAAAC,OAAO;sBAC9DkB,QAAA,CAAAG,YAAY,CAACtB,KAAA,CAAAC,OAAO,sDAfjCsB,UAAA,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}